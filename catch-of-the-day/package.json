{
  "name": "catch-of-the-day",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0",
    "firebase": "^7.0.0",
    "prop-types": "^15.0.0",
    "re-base": "^4.0.0",
    "react-router-dom": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
